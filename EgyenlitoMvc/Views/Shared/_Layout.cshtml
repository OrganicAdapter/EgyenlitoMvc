<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                <h1>Új Egyenlítő</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <nav>
                <ul id="menu">
                    <li>@Html.ActionLink("Újságok", "Index", "NewspaperManager")</li>
                    <li>@Html.ActionLink("Cikkek", "Index", "ArticleManager")</li>
                    <li>@Html.ActionLink("Események", "Index", "EventManager")</li>
                    <li>@Html.ActionLink("Támogatók", "Index", "UpholderManager")</li>
                    <li>@Html.ActionLink("Jelszó váltás", "ChangePassword", "Account")</li>
                </ul>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
